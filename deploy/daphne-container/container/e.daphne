time0 = now();

# Creating a random matrix
m = rand(51200, 512, 100.0, 200.0, 1.0, 42);
m2 = rand(1, 512, 100.0, 200.0, 1.0, 42);

# Do computation and measure time
time1 = now();
Z = m + m2;
time2 = now();

print("One demo sequence successfully completed. Step times: ");
print(time0);
print(time1);
print(time2);

print("\n\nComputed sum m+m2 is:");
print(sum(Z));
print("\n");


