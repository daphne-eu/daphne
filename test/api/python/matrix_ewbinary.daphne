m1 = [2.2];
m2 = [3.3];
s2 = 3.3;

# matrix op matrix.
print(m1 + m2);
print(m1 - m2);
print(m1 * m2);
print(m1 / m2);
print(m1 ^ m2);
print(m1 % m2);
print(m1 == m2);
print(m1 != m2);
print(m1 < m2);
print(m1 <= m2);
print(m1 > m2);
print(m1 >= m2);
print(m1 @ m2);
print(pow(m1, m2));
print(log(m1, m2));
print(mod(m1, m2));
print(min(m1, m2));
print(max(m1, m2));

# matrix op scalar.
print(m1 + s2);
print(m1 - s2);
print(m1 * s2);
print(m1 / s2);
print(m1 ^ s2);
print(m1 % s2);
print(m1 == s2);
print(m1 != s2);
print(m1 < s2);
print(m1 <= s2);
print(m1 > s2);
print(m1 >= s2);
print(pow(m1, s2));
print(log(m1, s2));
print(mod(m1, s2));
print(min(m1, s2));
print(max(m1, s2));

