m1 = reshape(seq(1, 100), 10, 10);

# Only row
print(m1[0, :]);
print(m1[0:5, :]);
print(m1[0:, :]);
print(m1[:5, :]);

# Only col
print(m1[:, 0]);
print(m1[:, 0:5]);
print(m1[:, 0:]);
print(m1[:, :5]);

# Row and col
print(m1[0:5, 0]);
print(m1[0, 0:5]);
print(m1[0, 0]);
print(m1[0:5, 0:5]);
print(m1[0:, 0]);
print(m1[0, 0:]);
print(m1[0:, 0:]);
print(m1[:5, 0]);
print(m1[0, :5]);
print(m1[:5, :5]);
