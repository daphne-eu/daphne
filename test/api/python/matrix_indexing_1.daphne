m1 = fill(123, 10, 10);

# Only row
print(m1[0, :]);
print(m1[0:5, :]);
print(m1[0:, :]);
print(m1[:5, :]);


# Only col
print(m1[:, 0]);
print(m1[:, 0:5]);
print(m1[:, 0:]);
print(m1[:, :5]);

#Row and col - corresponding variants
print(m1[0:5, 0]);
print(m1[0, 0:5]);
print(m1[0, 0]);
print(m1[0:5, 0:5]);
print(m1[0:, 0]);
print(m1[0, 0:]);
print(m1[0:, 0:]);
print(m1[:5, 0]);
print(m1[0, :5]);
print(m1[:5, :5]);
