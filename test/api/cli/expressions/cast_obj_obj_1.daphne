//-- Specify DataType Only

//matrix->frame
m0 = [0.1, 0.2];
print(as.frame(m0));

//frame->matrix
f0 = createFrame([20, 21, 22]);
print(as.matrix(f0));

//-- Specify ValueType Only

//matrix<f64>->matrix<si64>
m1 = [1.5];
print(as.si64(m1));

//matrix<si64>->matrix<f64>
m2 = [2];
print(as.f64(m2));

//-- Specify DataType and ValueType
f0 = createFrame([20, 21, 22]);
print(as.matrix<f64>(f0));
