// scientific notation (without decimal point)

// zero
print(0e0);
print(0e3);
print(0E0);
print(0E3);
print("");

// positive base, mixed exponent, small e
print(10e12);
print(1e6);
print(1e+6);
print(1e-6);
print("");
// capital E
print(10E12);
print(1E6);
print(1E+6);
print(1E-6);
print("");

// negative base, mixed exponent, small e
print(-10e12);
print(-1e6);
print(-1e+6);
print(-1e-6);
print("");
// capital E
print(-10E12);
print(-1E6);
print(-1E+6);
print(-1E-6);

