c1 = fill(123, 5, 1);
c2 = seq(100.5, 140.5, 10.0);
c3 = seq(-4, 4, 2);

f1 = createFrame(c1, c2, "a", "b");
f2 = createFrame(c3, "c");

registerView("x", f1);
registerView("y", f2);
print(f1);
print(f2);

s = sql("SELECT x.b, y.c FROM x, y WHERE x.a <> y.c AND x.b - 4.1 > 3.0 * 5.0 - 4.2;");

print(s);
