// Performs a MapOp with the UDF `topThree`. Used to compare precompiled
// kernel with codegen.

def topThree(col:matrix<f64>) -> matrix<f64> {
  return col[:3, 0];
}

X = reshape([1.1, 0.0, 5.5, -2.2, 1.1, 0.0, 5.5, -2.2, 1.1, 0.0, 5.5, -2.2, 1.1, 0.0, 5.5, -2.2], 4, 4);

print(map(X, topThree, 1));
