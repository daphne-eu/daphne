// binary '-' for subtraction, no spaces around the operator (used to be a problem in the past).

print("scalar - scalar");
print(1-2);
print(1.1-2.2);
print(1-2.3);

print("matrix - matrix");
print([1, 2]-[3, 4]);
print([1.1, 2.2]-[3.3, 4.4]);
print([1, 2]-[3.4, 4.6]);

print("matrix - scalar");
print([1, 2]-3);
print([1.1, 2.2]-3.3);
print([1, 2]-3.3);

print("scalar - matrix");
print(3-[1, 2]);
print(3.3-[1.1, 2.2]);
print(3.3-[1, 2]);