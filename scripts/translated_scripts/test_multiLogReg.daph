import "multiLogReg.daph";

n_samples = 3;
n_features = 2;
n_classes = 3;
seed = 1234;

X = rand(n_samples, n_features, 0.001, 1.0, 0.8, seed);
Y = rand(1, n_samples, 0, n_classes, 1.0 / n_classes, seed);
icpt = 2;
tol = 0.000001;
reg = 1.0;
maxi = 100;
maxii = 20;
verbose = 1;

multiLogReg.m_multiLogReg(X, as.matrix<f64>(Y), icpt, tol, reg, maxi, maxii, as.bool(verbose));
