m1 = rand(4, 1, 100.0, 200.0, 1.0, -1);
m2 = rand(4, 1, 0, 100, 1.0, -1);
f = createFrame(m1, m2, "a", "b");

m3 = rand(3, 1, 0, 2, 1.0, -1);
m4 = rand(3, 1, 100.0, 200.0, 1.0, -1);
h = createFrame(m3, m4, "c", "d");

print(f);
print(h);

registerView("x", f);
registerView("y", h);

k = sql("SELECT x.a, y.c, x.b FroM x, y as k;");

print(k);
